{
	"name": "market-overview",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"axios": "^0.20.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.5",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-scripts": "3.4.3"
	},
	"devDependencies": {
		"enzyme-to-json": "^3.4.4",
		"eslint": "6.6.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-react": "^7.20.3",
		"qa-utilities": "^1.1.0"
	},
	"jest": {
		"coveragePathIgnorePatterns": [
			"/src/serviceWorker.js",
			"/src/index.js"
		]
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "npm run test:scripts && npm run lint",
		"test:scripts": "react-scripts test --coverage --env=jsdom --setupFiles ./src/setupTests.js --watchAll=false",
		"test:scripts:watch": "react-scripts test --env=jsdom --setupFiles ./src/setupTests.js --coverage --watchAll=true",
		"lint": "eslint src/**/*.js src/**/*.jsx",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
